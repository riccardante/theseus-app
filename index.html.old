  <section role="region" id="splash">
    <div id="dSplash">
      <a class="recommend" id="bSignUp" role="button" href="#">Sign up</a>
      <a class="recommend" id="bSignIn" role="button" href="#">Sign in</a>
    </div>
  </section>


  <!-- SIGNIN -->
  <section role="region" id="signin">
    <header>
        <h1>KILOPORTA</h1>
    </header>
    <div id="dSignin">
    <form>
      <fieldset>
        <legend>Username</legend>
        <section>
          <p>
            <input type="text" placeholder="username" required="required"> 
         </p>
         </section>
       </fieldset>
      <fieldset>
        <legend>Password</legend>
        <section>
          <p>
            <input type="password" placeholder="xxxx" required="required"> 
         </p>
         </section>
       </fieldset>

      <a class="recommend" id="bFormSignIn" role="button" href="#">Sign in</a>
      </form>
    </div>
  </section>




